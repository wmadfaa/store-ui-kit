{
  "name": "store-ui-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen-snippets": "sb-playroom gen-snippets -c ./.playroom/babel.config.json -o ./.playroom/stories.snippets.json",
    "playroom:start": "npm run gen-snippets; playroom start",
    "playroom:build": "npm run gen-snippets; playroom build",
    "storybook-start-without-playroom": "start-storybook -p 9009 -s public",
    "storybook-build-without-playroom": "build-storybook -s public",
    "storybook:start": "run-p playroom:start storybook-start-without-playroom",
    "storybook:build": "run-p playroom:build storybook-build-without-playroom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "playroom": "^0.21.2",
    "react-scripts": "3.4.1",
    "storybook-addon-playroom": "^1.3.0"
  }
}